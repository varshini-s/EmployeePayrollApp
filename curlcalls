 curl -X PUT -H "Content-Type: application/json" -d '{"name": "Lisa","salary": 300}' "http://localhost:8080/employeepayrollservice/update/1" -w "\n"
 curl -X PUT -H "Content-Type: application/json" -d '{"name": "Lisa","salary": 3000}' "http://localhost:8080/employeepayrollservice/update/1" -w "\n"
 curl -X DELETE -H "Content-Type: application/json" localhost:8080/employeepayrollservice/delete/1 -w "\n"

